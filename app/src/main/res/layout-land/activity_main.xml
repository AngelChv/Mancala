<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/hole1Bttn"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        app:layout_constraintEnd_toStartOf="@+id/hole2Bttn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/hole2Bttn"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        app:layout_constraintEnd_toStartOf="@+id/hole3Bttn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/hole1Bttn"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/hole3Bttn"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        app:layout_constraintEnd_toStartOf="@+id/hole4Bttn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/hole2Bttn"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/hole4Bttn"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        app:layout_constraintEnd_toStartOf="@+id/rumaBttn"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/hole3Bttn"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/rumaBttn"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/hole4Bttn"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>